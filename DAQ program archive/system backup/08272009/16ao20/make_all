#!/bin/sh

echo driver ====================================================================
cd driver
make ${1}
./start
cd ..

echo docsrc ====================================================================
cd docsrc
make ${1}
cd ..

echo utils =====================================================================
cd utils
make ${1}
cd ..

echo aout ======================================================================
cd aout
make ${1}
cd ..

echo fsamp =====================================================================
cd fsamp
make ${1}
cd ..

echo id ========================================================================
cd id
make ${1}
cd ..

echo regs ======================================================================
cd regs
make ${1}
cd ..

echo sbtest ====================================================================
cd sbtest
make ${1}
cd ..

echo txrate ====================================================================
cd txrate
make ${1}
cd ..

